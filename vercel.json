{
  "version": 2,
  "routes": [
    {
      "src": "/gemini/v1beta/(.*)",
      "dest": "https://generativelanguage.googleapis.com/v1beta/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Authorization, Content-Type"
      }
    }
  ]
}