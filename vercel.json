{
  "version": 2,
  "routes": [
    {
      "src": "/openai/v1/(.*)",
      "dest": "https://api.openai.com/v1/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Authorization, Content-Type"
      }
    },
    {
      "src": "/gemini/v1beta/(.*)",
      "dest": "https://generativelanguage.googleapis.com/v1beta/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Authorization, Content-Type"
      }
    },
    {
      "src": "/claude/v1/(.*)",
      "dest": "https://api.anthropic.com/v1/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Authorization, Content-Type, x-api-key, anthropic-version"
      }
    },
    {
      "src": "/groq/openai/v1/(.*)",
      "dest": "https://api.groq.com/openai/v1/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Authorization, Content-Type"
      }
    },
    {
      "src": "/cohere/v2/(.*)",
      "dest": "https://api.cohere.ai/v2/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Authorization, Content-Type"
      }
    },
    {
      "src": "/xai/v1/(.*)",
      "dest": "https://api.x.ai/v1/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Authorization, Content-Type"
      }
    },
    {
      "src": "/mistral/v1/(.*)",
      "dest": "https://api.mistral.ai/v1/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Authorization, Content-Type"
      }
    },
    {
      "src": "/github/v1/(.*)",
      "dest": "https://ai.azure.com/github/v1/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Authorization, Content-Type"
      }
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}